{
  "name": "ts-a-star-maze-gen",
  "version": "0.0.1",
  "description": "main.bundle.js",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir docs --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext",
    "test": "karma start karma.config.js",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint --fix -p tsconfig.json"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "WholeTone Games",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jasmine": "2.8.7",
    "@types/node": "7.0.0",
    "@types/pixi.js": "^4.8.9",
    "awesome-typescript-loader": "5.2.0",
    "copy-webpack-plugin": "^5.0.4",
    "jasmine-core": "3.1.0",
    "karma": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "pixi.js": "^4.0.1",
    "puremvc-typescript-standard-framework": "git+https://github.com/Endemol/puremvc-typescript-standard-framework.git",
    "puremvc-typescript-util-asynccommand": "git+https://github.com/Endemol/puremvc-typescript-util-asynccommand.git",
    "source-map-loader": "0.2.3",
    "tslint": "^5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.5.1",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0"
  }
}